#装机后没有收入

#出库未装机
SELECT c.ccusname,c.finnal_ccusname,c.cinvname,c.ddate,d.new_installation_date
FROM 
(SELECT ccusname,finnal_ccusname,cinvname,ddate
FROM pdm.outdepot_order a
LEFT JOIN edw.x_cinv_relation b
ON b.cinvcode_child=a.cinvcode
WHERE b.level_two_cx='主设备' and YEAR(ddate)>2018)c
LEFT JOIN
#装机
(SELECT bi_cusname,bi_cinvcode,bi_cinvname,new_installation_date
FROM edw.crm_account_equipments a
WHERE YEAR(new_installation_date)>2018)d
ON c.finnal_ccusname=d.bi_cusname AND c.cinvname=d.bi_cinvname
WHERE /*d.new_installation_date is NULL OR */c.ddate>d.new_installation_date
